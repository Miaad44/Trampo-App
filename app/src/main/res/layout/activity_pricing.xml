<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Pricing">
    <ImageView
        android:id="@+id/imageView18"
        android:layout_width="401dp"
        android:layout_height="143dp"
        app:srcCompat="@drawable/logo" />
    <ImageView
        android:id="@+id/imageView19"
        android:layout_width="match_parent"
        android:layout_height="147dp"
        android:layout_below="@id/imageView18"
        android:layout_marginTop="-26dp"
        app:srcCompat="@drawable/pp" />
    <ImageView
        android:id="@+id/imageView20"
        android:layout_width="wrap_content"
        android:layout_height="366dp"
        android:layout_below="@id/imageView19"
        android:layout_marginTop="-92dp"
        app:srcCompat="@drawable/pricelist" />

    <ImageView
        android:id="@+id/imageView21"
        android:layout_width="match_parent"
        android:layout_height="227dp"
        android:layout_below="@id/imageView20"
        android:layout_marginTop="-176dp"
        app:srcCompat="@drawable/nnnn" />

</RelativeLayout>